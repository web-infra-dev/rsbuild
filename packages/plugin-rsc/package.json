{
  "name": "@rsbuild/plugin-rsc",
  "version": "1.4.0",
  "description": "React server component plugin for Rsbuild",
  "homepage": "https://rsbuild.rs",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rsbuild",
    "directory": "packages/plugin-rsc"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./rsc": {
      "types": "./dist/rsc.d.ts",
      "import": "./dist/rsc.mjs"
    },
    "./ssr": {
      "types": "./dist/ssr.d.ts",
      "import": "./dist/ssr.mjs"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "import": "./dist/client.mjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "compiled"
  ],
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build -w",
    "prebundle": "prebundle",
    "bump": "pnpx bumpp --no-tag"
  },
  "dependencies": {
  },
  "devDependencies": {
    "@rsbuild/core": "workspace:*",
    "@rslib/core": "0.18.4",
    "prebundle": "1.6.0",
    "react-server-dom-webpack": "^19.2.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "1.x",
    "react-server-dom-webpack": "^19.2.3"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
