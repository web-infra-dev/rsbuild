{
  "name": "@rsbuild/doctor-core",
  "version": "0.0.11",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rsbuild",
    "directory": "packages/doctor-core"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./build-utils": {
      "types": "./dist/build-utils/build/index.d.ts",
      "require": "./dist/build-utils/build/index.js",
      "import": "./dist/build-utils/build/index.js"
    },
    "./common-utils": {
      "types": "./dist/build-utils/common/index.d.ts",
      "require": "./dist/build-utils/common/index.js",
      "import": "./dist/build-utils/common/index.js"
    },
    "./plugins": {
      "types": "./dist/inner-plugins/index.d.ts",
      "require": "./dist/inner-plugins/index.js",
      "import": "./dist/inner-plugins/index.js"
    },
    "./rules": {
      "types": "./dist/rules/index.d.ts",
      "require": "./dist/rules/index.js",
      "import": "./dist/rules/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/types/index.js",
      "import": "./dist/types/index.js"
    }
  },
  "scripts": {
    "dev": "npm run start",
    "build": "modern build",
    "start": "modern build -w",
    "test": "vitest run"
  },
  "dependencies": {
    "@rsbuild/doctor-sdk": "workspace:*",
    "@rsbuild/doctor-utils": "workspace:*",
    "@rspack/core": "0.3.13",
    "axios": "^1.6.1",
    "bytes": "3.1.2",
    "enhanced-resolve": "5.12.0",
    "loader-utils": "^2.0.4",
    "lodash": "^4.17.21",
    "node-fetch": "^3.3.2",
    "path-browserify": "1.0.1",
    "semver": "^7.5.4",
    "source-map": "^0.7.4",
    "webpack-bundle-analyzer": "^4.9.1"
  },
  "devDependencies": {
    "@rsbuild/doctor-types": "workspace:*",
    "@rsbuild/test-helper": "workspace:*",
    "@types/bytes": "3.1.1",
    "@types/loader-utils": "^2.0.5",
    "@types/lodash": "^4.14.200",
    "@types/node": "^16",
    "@types/node-fetch": "^2.6.9",
    "@types/path-browserify": "1.0.1",
    "@types/semver": "^7.5.4",
    "@types/tapable": "2.2.2",
    "@types/webpack": "5.28.0",
    "babel-loader": "9.1.3",
    "string-loader": "0.0.1",
    "tslib": "2.4.1",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
