    "preact": "^10.28.2",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "@rsbuild/core": "^1.0.0 || ^2.0.0-0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
