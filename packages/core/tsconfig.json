{
  "extends": "@rsbuild/config/ts",
  "compilerOptions": {
    "outDir": "./dist",
    "baseUrl": "./",
    "rootDir": "src",
    "declarationDir": "./dist-types",
    "composite": true,
    "types": ["webpack/module"],
    "paths": {
      "open": ["../compiled/open"],
      "connect": ["../compiled/connect"],
      "commander": ["../compiled/commander"]
    }
  },
  "references": [
    {
      "path": "../shared"
    }
  ],
  "include": ["src"]
}
