{
  "name": "@rsbuild/core",
  "version": "1.0.12",
  "description": "The Rspack-based build tool.",
  "homepage": "https://rsbuild.dev",
  "bugs": {
    "url": "https://github.com/web-infra-dev/rsbuild/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rsbuild",
    "directory": "packages/core"
  },
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist-types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./client/hmr": {
      "types": "./dist-types/client/hmr.d.ts",
      "default": "./dist/client/hmr.js"
    },
    "./client/overlay": {
      "types": "./dist-types/client/overlay.d.ts",
      "default": "./dist/client/overlay.js"
    },
    "./types": {
      "types": "./types.d.ts"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/index.cjs",
  "types": "./dist-types/index.d.ts",
  "bin": {
    "rsbuild": "./bin/rsbuild.js"
  },
  "files": [
    "bin",
    "dist",
    "dist-types",
    "static",
    "compiled",
    "types.d.ts"
  ],
  "scripts": {
    "build": "modern build && tsc-alias -p tsconfig.json",
    "dev": "modern build --watch",
    "prebundle": "prebundle"
  },
  "dependencies": {
    "@rspack/core": "~1.0.9",
    "@rspack/lite-tapable": "~1.0.1",
    "@swc/helpers": "^0.5.13",
    "core-js": "~3.38.1"
  },
  "devDependencies": {
    "@types/connect": "3.4.38",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "18.x",
    "@types/on-finished": "2.3.4",
    "@types/webpack-bundle-analyzer": "4.7.0",
    "@types/ws": "^8.5.12",
    "browserslist-load-config": "0.1.1",
    "chokidar": "3.6.0",
    "commander": "^12.1.0",
    "connect": "3.7.0",
    "connect-history-api-fallback": "^2.0.0",
    "css-loader": "7.1.2",
    "deepmerge": "^4.3.1",
    "dotenv": "16.4.5",
    "dotenv-expand": "11.0.6",
    "fs-extra": "^11.2.0",
    "html-rspack-plugin": "6.0.1",
    "http-proxy-middleware": "^2.0.6",
    "jiti": "^1.21.6",
    "launch-editor-middleware": "^2.9.1",
    "mrmime": "^2.0.0",
    "on-finished": "2.4.1",
    "open": "^8.4.0",
    "picocolors": "^1.1.0",
    "postcss": "^8.4.47",
    "postcss-load-config": "6.0.1",
    "postcss-loader": "8.1.1",
    "prebundle": "1.2.2",
    "reduce-configs": "^1.0.0",
    "rsbuild-dev-middleware": "0.1.1",
    "rslog": "^1.2.3",
    "rspack-chain": "^1.0.3",
    "rspack-manifest-plugin": "5.0.1",
    "sirv": "^2.0.4",
    "style-loader": "3.3.4",
    "tsc-alias": "^1.8.10",
    "typescript": "^5.6.3",
    "webpack": "^5.95.0",
    "webpack-bundle-analyzer": "^4.10.2",
    "webpack-merge": "6.0.1",
    "ws": "^8.18.0"
  },
  "optionalDependencies": {
    "fsevents": "~2.3.3"
  },
  "engines": {
    "node": ">=16.7.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/"
  }
}
