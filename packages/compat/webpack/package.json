{
  "name": "@rsbuild/webpack",
  "version": "0.0.16",
  "homepage": "https://rsbuild.dev",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rsbuild",
    "directory": "packages/compat/webpack"
  },
  "license": "MIT",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./plugin-css": {
      "types": "./dist/plugins/css.d.ts",
      "default": "./dist/plugins/css.js"
    },
    "./plugin-babel": {
      "types": "./dist/plugins/babel.d.ts",
      "default": "./dist/plugins/babel.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./src/index.ts",
  "typesVersions": {
    "*": {
      "plugin-babel": [
        "./dist/plugins/babel.d.ts"
      ],
      "plugin-css": [
        "./dist/plugins/css.d.ts"
      ]
    }
  },
  "files": [
    "static",
    "compiled",
    "dist"
  ],
  "scripts": {
    "build": "modern build",
    "dev": "modern build --watch"
  },
  "dependencies": {
    "@babel/core": "^7.23.2",
    "@babel/preset-react": "^7.22.15",
    "@modern-js/server": "0.0.0-next-20231103131234",
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.10",
    "@rsbuild/babel-preset": "workspace:*",
    "@rsbuild/core": "workspace:*",
    "@rsbuild/plugin-babel": "workspace:*",
    "@rsbuild/plugin-css-minimizer": "workspace:*",
    "@rsbuild/shared": "workspace:*",
    "babel-loader": "9.1.3",
    "babel-plugin-import": "1.13.5",
    "babel-plugin-transform-react-remove-prop-types": "0.4.24",
    "caniuse-lite": "^1.0.30001559",
    "html-webpack-plugin": "5.5.3",
    "lodash": "^4.17.21",
    "mini-css-extract-plugin": "2.7.6",
    "postcss": "8.4.31",
    "react-refresh": "^0.14.0",
    "style-loader": "3.3.3",
    "terser-webpack-plugin": "5.3.9",
    "ts-loader": "9.4.4",
    "tsconfig-paths-webpack-plugin": "4.1.0",
    "webpack": "^5.89.0",
    "webpack-subresource-integrity": "5.1.0"
  },
  "devDependencies": {
    "@rsbuild/test-helper": "workspace:*",
    "@types/lodash": "^4.14.200",
    "@types/node": "^16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true,
    "registry": "https://registry.npmjs.org/",
    "types": "./dist/index.d.ts"
  }
}
