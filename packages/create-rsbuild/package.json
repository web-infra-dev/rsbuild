{
  "name": "create-rsbuild",
  "version": "2.0.0-beta.6",
  "description": "Create a new Rsbuild project",
  "homepage": "https://rsbuild.rs",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/web-infra-dev/rsbuild",
    "directory": "packages/create-rsbuild"
  },
  "bin": {
    "create-rsbuild": "./bin.js"
  },
  "files": [
    "template-*",
    "dist",
    "bin.js"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "rslib build",
    "dev": "rslib build -w",
    "start": "node ./dist/index.js",
    "bump": "pnpx bumpp --no-tag"
  },
  "dependencies": {
    "create-rstack": "1.8.0"
  },
  "devDependencies": {
    "@rsbuild/core": "workspace:*",
    "@rsbuild/plugin-babel": "workspace:*",
    "@rsbuild/plugin-preact": "workspace:*",
    "@rsbuild/plugin-react": "workspace:^1.1.0",
    "@rsbuild/plugin-solid": "workspace:*",
    "@rsbuild/plugin-svelte": "workspace:*",
    "@rsbuild/plugin-vue": "workspace:*",
    "@rslib/core": "0.19.6",
    "@types/node": "^24.11.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": "^20.19.0 || >=22.12.0"
  }
}
