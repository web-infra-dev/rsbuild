{
  "extends": "@rsbuild/tsconfig/base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "outDir": "./dist",
    "paths": {
      "@theme": ["./theme"],
      "@zh/*": ["./docs/zh/*"],
      "@en/*": ["./docs/en/*"]
    },
    "baseUrl": "./"
  },
  "mdx": {
    "checkMdx": true
  },
  "include": ["docs", "rspress.config.ts", "theme", "components"]
}
