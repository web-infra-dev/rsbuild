{
  "private": true,
  "name": "@rsbuild/e2e",
  "type": "module",
  "version": "1.0.0",
  "scripts": {
    "setup": "pnpm exec playwright install chromium --with-deps",
    "e2e": "pnpm e2e:rspack && pnpm e2e:webpack && pnpm type-check",
    "e2e:rspack": "cross-env NODE_OPTIONS=\"--experimental-vm-modules --no-warnings\" playwright test",
    "e2e:webpack": "cross-env PROVIDE_TYPE=webpack playwright test",
    "type-check": "tsc"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "moment": "^2.30.1",
    "preact": "^10.27.2",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-router-dom": "^7.9.6",
    "solid-js": "^1.9.10",
    "vue": "^3.5.25",
    "vue-router": "^4.6.3"
  },
  "devDependencies": {
    "@e2e/assets": "workspace:*",
    "@e2e/helper": "workspace:*",
    "@module-federation/enhanced": "0.21.6",
    "@module-federation/rsbuild-plugin": "0.21.6",
    "@playwright/test": "1.57.0",
    "@rsbuild/core": "workspace:*",
    "@rsbuild/plugin-babel": "workspace:*",
    "@rsbuild/plugin-check-syntax": "^1.6.0",
    "@rsbuild/plugin-less": "workspace:*",
    "@rsbuild/plugin-preact": "workspace:*",
    "@rsbuild/plugin-react": "workspace:*",
    "@rsbuild/plugin-rem": "^1.0.4",
    "@rsbuild/plugin-sass": "workspace:*",
    "@rsbuild/plugin-solid": "workspace:*",
    "@rsbuild/plugin-stylus": "workspace:*",
    "@rsbuild/plugin-svelte": "workspace:*",
    "@rsbuild/plugin-svgr": "workspace:*",
    "@rsbuild/plugin-type-check": "^1.3.1",
    "@rsbuild/plugin-vue": "workspace:*",
    "@rsbuild/plugin-vue-jsx": "^1.1.1",
    "@rsbuild/plugin-webpack-swc": "workspace:*",
    "@rsbuild/webpack": "workspace:*",
    "@rsdoctor/rspack-plugin": "1.3.11",
    "@scripts/test-helper": "workspace:*",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.21",
    "@types/node": "^24.10.1",
    "@types/polka": "^0.5.8",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "create-rsbuild": "workspace:*",
    "fast-glob": "^3.3.3",
    "fs-extra": "^11.3.2",
    "playwright": "1.57.0",
    "polka": "^0.5.2",
    "tailwindcss": "^4.1.17",
    "typescript": "^5.9.3"
  }
}
